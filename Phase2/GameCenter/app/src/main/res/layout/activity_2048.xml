<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GameActivity">

    <fall2018.csc2017.slidingtiles.GestureDetectGridView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/TFEgrid"
        android:layout_width="wrap_content"
        android:layout_height="580dp"
        android:layout_above="@+id/fragment"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="8dp"
        tools:context="fall2018.csc2017.TwentyFortyEight.GameActivity">

    </fall2018.csc2017.slidingtiles.GestureDetectGridView>

    <fragment
        android:id="@+id/fragment"
        android:name="fall2018.csc2017.GameManager.GameButtons"
        android:layout_width="match_parent"
        android:layout_height="67dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="0dp" />


</RelativeLayout>